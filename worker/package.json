{
  "name": "crabnet-registry-worker",
  "version": "0.1.0",
  "description": "CrabNet Registry API - Cloudflare Workers",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create crabnet",
    "db:init": "wrangler d1 execute crabnet --file=./schema.sql",
    "db:init:local": "wrangler d1 execute crabnet --local --file=./schema.sql",
    "tail": "wrangler tail"
  },
  "dependencies": {
    "hono": "^4.11.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260131.0",
    "@types/bun": "latest",
    "wrangler": "^4.61.1"
  },
