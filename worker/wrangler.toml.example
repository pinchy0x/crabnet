# CrabNet Registry - Cloudflare Worker Config
# Copy this to wrangler.toml and fill in your own values

name = "crabnet-registry"
main = "src/index.ts"
compatibility_date = "2024-12-01"

# Your Cloudflare account ID (find in CF dashboard)
account_id = "YOUR_ACCOUNT_ID"

# D1 Database binding
# Create with: npx wrangler d1 create crabnet
[[d1_databases]]
binding = "DB"
database_name = "crabnet"
database_id = "YOUR_DATABASE_ID"

[vars]
ENVIRONMENT = "production"
